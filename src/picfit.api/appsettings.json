{
  "storage": {
    "type": "fs", // fs
    "location": "./store" // path/to/directory
  },
  "cache": {
    "type": "fs",
    "location":  "./store/cache"
  },
  "imageprocessing": {
    "type": "imagesharp", // imagesharp
    "scales": [
      75,
      100,
      150,
      200
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
